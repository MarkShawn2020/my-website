{
  "name": "my-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "clear:tsc": "cd config && tsc -b --clean",
    "start:tsc": "cd config && tsc && cd - && docusaurus start --config config/src/index.js",
    "dev": "yarn start",
    "restart": "rm -rf .docusaurus && rm -rf node_modules/.cache && docusaurus start",
    "update-version": "yarn run docusaurus docs:version $(cat VERSION.txt)",
    "build": "docusaurus build",
    "build:config": "webpack --config config/webpack.config.ts",
    "rebuild": "rm -rf node_modules/.cache && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear && cd config && tsc -b --clean",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@docusaurus/preset-classic": "3.0.0-alpha.0",
    "@mdx-js/react": "^1.6.22",
    "antd": "^4.22.3",
    "clsx": "^1.2.1",
    "lodash": "^4.17.21",
    "mdx-mermaid": "^1.2.3",
    "mermaid": "^9.1.4",
    "module-alias": "^2.2.2",
    "prism-react-renderer": "^1.3.5",
    "react": "*",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "*",
    "react-draggable": "^4.4.5",
    "rehype-katex": "4",
    "remark-math": "3",
    "tsconfig-paths": "^4.1.0",
    "ts-node": "^10.9.1",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/antd": "^1.0.0",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.7.2",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "esbuild-loader": "^2.19.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "tsc-alias": "^1.7.0",
    "ttypescript": "^1.5.13"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
